# PostHog API Configuration
# Required for all operations
POSTHOG_PERSONAL_API_KEY=phx_your_personal_api_key_here
POSTHOG_PROJECT_ID=12345

# Required only for event capture and feature flag evaluation
POSTHOG_PROJECT_API_KEY=phc_your_project_api_key_here

# Optional: API URL (defaults to US cloud)
# For EU cloud, use: https://eu.posthog.com
# For self-hosted, use your instance URL
POSTHOG_API_URL=https://us.posthog.com

# E2B Configuration (for sandbox execution)
E2B_API_KEY=your_e2b_api_key_here
